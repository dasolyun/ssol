        for (var i = 0; i < divs.length; i++) {


            $(".slider_text_price2").append("<p>" + "판매가　　　" + "<span>" + msg.documents[0].sale_price.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") + "</span>" + "원" + "<span>" + " (10%할인)" + "</span>" + "</p>");

        }



    });



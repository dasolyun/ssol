$.ajax({
        method: "GET",
        url: "https://dapi.kakao.com/v3/search/book?target=title",
        data: {
            query: "css3"
        },
        headers: {
            Authorization: "KakaoAK 5fbb93db070c5b2131d6bb6d20a34376"
        }
    })
    .done(function (msg) {

        console.log(msg.documents[1].title);
        console.log(msg.documents[1].thumbnail);







        // for문 (8개)
        var divs = document.getElementsByClassName('slider_text_authors1');

        for (var i = 0; i < divs.length; i++) {


            $(".slider_text_authors1").append("<p>" + "<span>" + "저자: " + "</span>" + msg.documents[0].authors[0] + "　|　" + "</p>");
            $(".slider_text_authors1").append("<p>" + "<span>" + " 출판사: " + "</span>" + msg.documents[0].publisher + "　|　" + "</p>");
            $(".slider_text_authors1").append("<p>" + "<span>" + "역자: " + "</span>" + msg.documents[0].translators + "</p>");
        }



    });




$.ajax({
        method: "GET",
        url: "https://dapi.kakao.com/v3/search/book?target=title",
        data: {
            query: "css3"
        },
        headers: {
            Authorization: "KakaoAK 5fbb93db070c5b2131d6bb6d20a34376"
        }
    })
    .done(function (msg) {

        console.log(msg.documents[1].title);
        console.log(msg.documents[1].thumbnail);







        // for문 (8개)
        var divs = document.getElementsByClassName('slider_text_authors2');

        for (var i = 0; i < divs.length; i++) {


            $(".slider_text_authors2").append("<p>" + msg.documents[0].status + "　|　" + "</p>");
            $(".slider_text_authors2").append("<p>" + msg.documents[0].datetime + "　|　" + "</p>");
            $(".slider_text_authors2").append("<p>" + " ISBN-13: " + msg.documents[0].isbn + "</p>");

        }



    });


$.ajax({
        method: "GET",
        url: "https://dapi.kakao.com/v3/search/book?target=title",
        data: {
            query: "css3"
        },
        headers: {
            Authorization: "KakaoAK 5fbb93db070c5b2131d6bb6d20a34376"
        }
    })
    .done(function (msg) {

        console.log(msg.documents[1].title);
        console.log(msg.documents[1].thumbnail);







        // for문 (8개)
        var divs = document.getElementsByClassName('slider_text_price1');

        for (var i = 0; i < divs.length; i++) {


            $(".slider_text_price1").append("<p>" + "정가　　　　" + "<span>" + msg.documents[0].price.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",") + "</span>" + "원" + "</p>");

        }



    });

